{
    "LaunchTemplateName": "EC2ContainerService-EcsOptAMI2-t3med",
    "VersionDescription": "test description",
    "LaunchTemplateData": {
        "EbsOptimized": false,
        "IamInstanceProfile": {
            "Arn": "$IamInstanceProfile"
        },
        "ImageId": "$AwsImageId", 
        "InstanceType": "$AwsInstanceType",
        "KeyName": "$sshKeyName",
        "Monitoring": {
            "Enabled": true
        },
        "UserData": "",
        "TagSpecifications": [
            {
                "ResourceType": "instance",
                "Tags": [
                    {
                        "Key": "team",
                        "Value": "datascience"
                    },
                    {
                        "Key": "name",
                        "Value": "ds-team-instance"
                    }                    
                ]
            }
        ],
        "SecurityGroupIds": [
            "$sgserver"
        ],
        "Placement": {
            "Tenancy": "dedicated"
        },
        "CreditSpecification": {
            "CpuCredits": "unlimited"
        },
        "MetadataOptions": {
            "HttpTokens": "required",
            "HttpPutResponseHopLimit": 1,
            "HttpEndpoint": "enabled"
        }        
    }
}
