{
    "LaunchTemplateName": "EC2ContainerService-EcsOptAMI2-t3med",
    "VersionDescription": "test description",
    "LaunchTemplateData": {
        "EbsOptimized": false,
        "IamInstanceProfile": {
            "Arn": "$IamInstanceProfile"
        },
        "BlockDeviceMappings": [
            {
                "DeviceName": "/dev/sda1",
                "Ebs": {
                    "VolumeSize": $EbsVolumeSize,
                    "DeleteOnTermination": $EbsDeleteOnTermination
                }
            }    
        ],               
        "ImageId": "$AwsImageId", 
        "InstanceType": "$AwsInstanceType",
        "KeyName": "$sshKeyName",
        "Monitoring": {
            "Enabled": true
        },
        "UserData": "",
        "NetworkInterfaces": [
            {
                "AssociatePublicIpAddress": true,
                "DeviceIndex": 0,
                "SubnetId": "$subnet"
            }
        ],
        "TagSpecifications": [
            {
                "ResourceType": "instance",
                "Tags": [
                    {
                        "Key": "team",
                        "Value": "datascience"
                    },
                    {
                        "Key": "name",
                        "Value": "ds-team-instance"
                    }                    
                ]
            }
        ],
        "SecurityGroupIds": [
            "$sgserver"
        ]
    }
}
